{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "tab-section/tab-section",
	"version": "0.1.0",
	"title": "Tab section",
	"category": "design",
	"icon": "smiley",
	"description": "Simple group with tab",
	"example": {},
	"supports": {
		"html": false,
		"spacing": {
			"padding":true,
			"margin":true
		}
	},
	"attributes": {
		"tagName":{
			"type":"string",
			"default":"h3"
		},
		"tagCopy":{
			"type":"string",
			"default":"label"
		}
	},
	"textdomain": "tab-section",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"render": "file:./render.php",
	"viewScript": "file:./view.js"
}
